const { Command_template } = require("../config/templates");

class Command extends Command_template {
  constructor(args, interaction) {
    super(interaction);
    Object.assign(this, args);

    this.options = {
      permissions: [],
      custom_perms: [],
      slash: {
        name: "COMMAND_NAME",
        description: "COMMAND_DESCRIPTION",
      }
    };
  }

  async execute() {}
}

module.exports = Command;
